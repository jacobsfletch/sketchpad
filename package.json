{
  "name": "sketchpad",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/components/ -o src/components",
    "watch-css": "npm run build-css && node-sass-chokidar src/components/ -o src/components/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "PORT=8080 npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
